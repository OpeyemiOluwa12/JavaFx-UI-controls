<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>


<StackPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.opeyemi.javafx.combobox.controller.ComboboxController">
   <children>
      <VBox>
         <children>
            <HBox alignment="CENTER_LEFT" spacing="20.0">
               <children>
                  <Text fill="#575454" strokeType="OUTSIDE" strokeWidth="0.0" text="List of Tailors Products">
                     <font>
                        <Font name="Work Sans Medium" size="14.0" />
                     </font>
                  </Text>
                  <ComboBox fx:id="productComboBox" maxWidth="200.0" minWidth="100.0" promptText="Select products" style="-fx-text-fill: #575454; -fx-font-family: &quot;Work Sans Medium&quot;; -fx-font-size: 16px;" HBox.hgrow="ALWAYS" />
               </children>
               <padding>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </padding>
            </HBox>
         </children>
      </VBox>
   </children>
</StackPane>
